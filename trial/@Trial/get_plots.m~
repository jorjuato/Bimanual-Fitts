function [fcns, names, xlabels, ylabels] = get_plots(obj)
    idx=[1,2,3,6];
    fcns = {...
         @plot_time_series...
        ,@plot_phase_plane...
        ,@plot_hook_plane...
        ,@plot_relative_phase...
        ,@plot_relative_phase_hist...
        }; fcns=fcns{idx};

    names = {...
        'time_series'...
        ,'phase_plane'...
        ,'hook_plane'...
        ,'relative_phase'...
        ,'relative phase histogram'...
        ,'frequencies'...
        ,'histogram of Rx - Lx'...
        ,'triple mixed normal distribution fitting'...
        ,'Spectrogram of combined signals'...
        ,'Instantaneous frequency'...
        }; names=names{idx};

    ylabels = {...
        'Position/Speed(m;m/s)'...
        ,'Speed(m/s)'...
        ,'Accel (m/s^2)'...
        ,'Phase Diff (rad)'...
        ,'proportion'...
        ,'Power(dB)'...
        ,'Distance (m)'...
        ,'Distance (m)'...
        ,'Frequency (Hz)'...
        ,'Frequency (Hz)'...
        }; ylabels=ylabels{idx};

    xlabels = {...
        'Time (ms)'...
        ,'Position (m)'...
        ,'Position (m)'...
        ,'Time (ms)'...
        ,'Relative Phase (rad)'...
        ,'Frequency (Hz)'...
        ,'Time (ms)'...
        ,'probability'...
        ,'Time (ms)'...
        ,'Time (ms)'...
        }; xlabels=xlabels{idx};
end